#!/bin/bash

unzip "$1"
rm cabal2json-{darwin,linux,win32.exe}

tar Jxvf cabal2json-Linux.tar.xz
mv cabal2json{,-linux}
rm cabal2json-Linux.tar.xz

tar Jxvf cabal2json-macOS.tar.xz
mv cabal2json{,-darwin}
rm cabal2json-macOS.tar.xz

7z x cabal2json-Windows.7z
mv build-Windows/cabal2json.exe cabal2json-win32.exe
rmdir build-Windows
rm cabal2json-Windows.7z

chmod +x cabal2json-{darwin,linux,win32.exe}
